{
  "name": "ps-dev-toolbox",
  "displayName": "Dev Toolbox",
  "version": "0.0.21",
  "publisher": "playful-sparkle",
  "description": "All-in-one toolset for URL slug generation, Base64/URL encoding/decoding, GUID generation, and removing empty lines, non-printable characters, whitespace and many more.",
  "author": {
    "name": "Zsolt Oroszl√°ny",
    "email": "hello@playfulsparkle.com"
  },
  "categories": [
    "Formatters",
    "Other"
  ],
  "icon": "img/icon.png",
  "galleryBanner": {
    "color": "#e61a4a",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/visual-studio-marketplace/v/playful-sparkle.ps-dev-toolbox?style=flat-square",
      "href": "https://marketplace.visualstudio.com/items?itemName=playful-sparkle.ps-dev-toolbox",
      "description": "The latest version of this extension"
    },
    {
      "url": "https://img.shields.io/visual-studio-marketplace/i/playful-sparkle.ps-dev-toolbox?style=flat-square",
      "href": "https://marketplace.visualstudio.com/items?itemName=playful-sparkle.ps-dev-toolbox",
      "description": "Number of installs for this extension"
    },
    {
      "url": "https://img.shields.io/visual-studio-marketplace/stars/playful-sparkle.ps-dev-toolbox?style=flat-square",
      "href": "https://marketplace.visualstudio.com/items?itemName=playful-sparkle.ps-dev-toolbox",
      "description": "Number of stars for this extension"
    }
  ],
  "pricing": "Free",
  "engines": {
    "vscode": "^1.98.0"
  },
  "main": "dist/extension.js",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playfulsparkle/vscode_ps_dev_toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/playfulsparkle/vscode_ps_dev_toolbox/issues"
  },
  "homepage": "https://github.com/playfulsparkle/vscode_ps_dev_toolbox#readme",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "keywords": [
    "slugify",
    "slug",
    "base64",
    "encode",
    "decode",
    "url-encode",
    "url-decode",
    "guid",
    "uuid",
    "camelcase",
    "pascalcase",
    "snakecase",
    "kebabcase",
    "train-case",
    "flatcase",
    "uppercase",
    "lowercase",
    "text-formatting",
    "trim",
    "non-printable",
    "sort",
    "sorting",
    "html-entities",
    "html-encode",
    "html-decode",
    "escape",
    "text-utilities",
    "developer-tools",
    "productivity",
    "playfulsparkle",
    "vscode",
    "extension"
  ],
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "ps-dev-toolbox.slugifyFileOrFolder",
        "title": "%extension.command.slugifyFileOrFolder.title%"
      },
      {
        "command": "ps-dev-toolbox.toCamelCase",
        "title": "%extension.command.toCamelCase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.toPascalCase",
        "title": "%extension.command.toPascalCase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.toSnakeCase",
        "title": "%extension.command.toSnakeCase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.toScreamingSnakeCase",
        "title": "%extension.command.toScreamingSnakeCase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.toKebabCase",
        "title": "%extension.command.toKebabCase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.toTrainCase",
        "title": "%extension.command.toTrainCase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.toFlatCase",
        "title": "%extension.command.toFlatCase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.toUpperCase",
        "title": "%extension.command.toUpperCase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.makeLowercase",
        "title": "%extension.command.makeLowercase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.makeUppercase",
        "title": "%extension.command.makeUppercase.title%",
        "category": "%extension.category.textFormat.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.slugify",
        "title": "%extension.command.slugify.title%",
        "category": "%extension.category.utilities.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.generateGuid",
        "title": "%extension.command.generateGuid.title%",
        "category": "%extension.category.utilities.title%"
      },
      {
        "command": "ps-dev-toolbox.removeEmptyLinesDocument",
        "title": "%extension.command.removeEmptyLinesDocument.title%",
        "category": "%extension.category.utilities.title%"
      },
      {
        "command": "ps-dev-toolbox.removeEmptyLinesSelection",
        "title": "%extension.command.removeEmptyLinesSelection.title%",
        "category": "%extension.category.utilities.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.cleanText",
        "title": "%extension.command.cleanText.title%",
        "category": "%extension.category.utilities.title%"
      },
      {
        "command": "ps-dev-toolbox.trimLineWhitespace",
        "title": "%extension.command.trimLineWhitespace.title%",
        "category": "%extension.category.utilities.title%"
      },
      {
        "command": "ps-dev-toolbox.sortLinesAscending",
        "title": "%extension.command.sortLinesAscending.title%",
        "category": "%extension.category.utilities.title%"
      },
      {
        "command": "ps-dev-toolbox.sortLinesDescending",
        "title": "%extension.command.sortLinesDescending.title%",
        "category": "%extension.category.utilities.title%"
      },
      {
        "command": "ps-dev-toolbox.base64Encode",
        "title": "%extension.command.base64Encode.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.base64Decode",
        "title": "%extension.command.base64Decode.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.urlEncode",
        "title": "%extension.command.urlEncode.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.urlDecode",
        "title": "%extension.command.urlDecode.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.encodeHTMLNamedCharacterEntitys",
        "title": "%extension.command.encodeHTMLNamedCharacterEntitys.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.decodeHTMLNamedCharacterEntitys",
        "title": "%extension.command.decodeHTMLNamedCharacterEntitys.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.encodeHTMLHexadecimalCharacterReference",
        "title": "%extension.command.encodeHTMLHexadecimalCharacterReference.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.decodeHTMLHexadecimalCharacterReference",
        "title": "%extension.command.decodeHTMLHexadecimalCharacterReference.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.encodeHtmlDecimalEntities",
        "title": "%extension.command.encodeHtmlDecimalEntities.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.decodeHtmlDecimalEntities",
        "title": "%extension.command.decodeHtmlDecimalEntities.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.encodeJavaScriptUTF16EscapeSequences",
        "title": "%extension.command.encodeJavaScriptUTF16EscapeSequences.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.decodeJavaScriptUTF16EscapeSequences",
