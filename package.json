        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.decodeUnicodeCodePointNotation",
        "title": "%extension.command.decodeUnicodeCodePointNotation.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.encodeUnicodeCodePointEscapeSequence",
        "title": "%extension.command.encodeUnicodeCodePointEscapeSequence.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.decodeUnicodeCodePointEscapeSequence",
        "title": "%extension.command.decodeUnicodeCodePointEscapeSequence.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.encodePCREUnicodeHexadecimalEcape",
        "title": "%extension.command.encodePCREUnicodeHexadecimalEcape.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.decodePCREUnicodeHexadecimalEcape",
        "title": "%extension.command.decodePCREUnicodeHexadecimalEcape.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.encodeHexCodePoints",
        "title": "%extension.command.encodeHexCodePoints.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.decodeHexCodePoints",
        "title": "%extension.command.decodeHexCodePoints.title%",
        "category": "%extension.category.encoding.title%",
        "enablement": "editorHasSelection || editorHasMultipleSelections"
      },
      {
        "command": "ps-dev-toolbox.reportIssue",
        "title": "%extension.command.reportIssue.title%",
        "category": "%extension.category.help.title%"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ps-dev-toolbox.slugifyFileOrFolder",
          "group": "7_modification",
          "when": "explorerResourceIsFolder == false"
        },
        {
          "command": "ps-dev-toolbox.slugifyFileOrFolder",
          "group": "7_modification",
          "when": "explorerResourceIsFolder == true"
        }
      ],
      "issue/reporter": [
        {
          "command": "ps-dev-toolbox.reportIssue"
        }
      ]
    },
    "configuration": {
      "title": "Dev Toolbox",
      "properties": {
        "ps-dev-toolbox.trimLineWhitespace.preserveTabs": {
          "type": "boolean",
          "default": true,
          "description": "%extension.config.trimLineWhitespace.preserveTabs.desc%"
        },
        "ps-dev-toolbox.removeEmptyLines.whiteSpaceEmpty": {
          "type": "boolean",
          "default": true,
          "description": "%extension.config.removeEmptyLines.whiteSpaceEmpty.desc%"
        },
        "ps-dev-toolbox.encoding.doubleEncode": {
          "type": "boolean",
          "default": false,
          "description": "%extension.config.encoding.doubleEncode.desc%"
        },
        "ps-dev-toolbox.slugify.separator": {
          "type": "string",
          "default": "-",
          "description": "%extension.config.slugify.desc%",
          "enum": [
            "-",
            "_"
          ],
          "enumDescriptions": [
            "%extension.config.slugify.dash.desc%",
            "%extension.config.slugify.underscore.desc%"
          ]
        },
        "ps-dev-toolbox.uuid.format": {
          "type": "string",
          "default": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
          "description": "%extension.config.uuid.desc%",
          "enum": [
            "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
            "{xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx}",
            "[Guid(\"xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx\")]",
            "<Guid(\"xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx\")>"
          ],
          "enumDescriptions": [
            "%extension.config.uuid.raw.desc%",
            "%extension.config.uuid.registryFormat.desc%",
            "%extension.config.uuid.squareFormat.desc%",
            "%extension.config.uuid.lessThanSignFormat.desc%"
          ]
        },
        "ps-dev-toolbox.sorting.numeric": {
          "type": "boolean",
          "default": true,
          "description": "%extension.config.sorting.numeric%"
        },
        "ps-dev-toolbox.sorting.ignoreCase": {
          "type": "boolean",
          "default": true,
          "description": "%extension.config.sorting.ignoreCase%"
        },
        "ps-dev-toolbox.sorting.locale": {
          "type": "string",
          "default": "",
          "description": "%extension.config.sorting.locale%"
        }
      }
    },
    "keybindings": []
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.1",
    "eslint": "^9.23.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "dependencies": {
    "@vscode/l10n": "^0.0.18"
  }
}